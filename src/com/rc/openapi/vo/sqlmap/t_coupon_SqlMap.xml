   <isNotNull property="member_id" >
    (SELECT COUNT(cc.id) FROM t_coupon_card cc where cc.ticket_id=c.id and cc.member_id= $member_id$ and  DATE_FORMAT(cc.create_time,'%Y%m%d')=DATE_FORMAT(now(),'%Y%m%d')) AS isget
   </isNotNull>
   <isNull property="member_id" >
    0 AS isget
   </isNull>   
   FROM t_coupon c
   WHERE c.id IN ($cp$) AND c. STATUS = 1  AND c.end_time &gt;= NOW()
