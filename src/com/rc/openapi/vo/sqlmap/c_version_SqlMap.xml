<select id="check_version" parameterClass="java.util.Map" resultClass="java.lang.Integer" >
   	SELECT COUNT(0) as cnt
   	FROM c_version v
   	WHERE v.id &gt; (SELECT t.id FROM c_version t WHERE t.version_info='$vers$' AND t.type=#t:decimal#) and v.type=#t:decimal#
 </select>
 
 <select id="get-comment" parameterClass="java.util.Map" resultClass="java.lang.String">
SELECT c.`comment` FROM c_version c WHERE c.id>(
SELECT t.id FROM c_version t  WHERE t.version_info = '$vers$' AND t.type = #t:decimal#   
) AND c.type =  #t:decimal# 
 </select>
 
 
<select id="get-new-record" parameterClass="java.util.Map" resultMap="ibatorgenerated_BaseResultMap" >
SELECT c.* FROM c_version c WHERE c.id>(
		SELECT t.id FROM c_version t  WHERE t.version_info = '$vers$' AND t.type = #t:decimal#   
) AND c.type =  #t:decimal# 
order by c.id desc
</select>
 

  
